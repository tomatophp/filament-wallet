{
    "name": "tomatophp/filament-wallet",
    "type": "library",
    "description": "Account Balance / Wallets Manager For FilamentPHP and Filament Account Builder",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentWallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentWallet\\FilamentWalletServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "bavix/laravel-wallet": "^10.0",
        "filament/filament": "^3.0.0"
    }
}
